{
    "name": "gitbook-plugin-theme-next",
    "description": "Wikia Theme",
    "main": "./index.js",
    "version": "1.0.4",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "devDependencies": {
        "browser-sync": "^2.18.12",
        "browserify": "13.1.0",
        "font-awesome": "4.6.3",
        "gulp": "github:gulpjs/gulp#4.0",
        "gulp-browserify": "^0.5.1",
        "gulp-concat": "^2.6.1",
        "gulp-exec": "^2.1.3",
        "gulp-ignore": "^2.0.2",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "jquery": "3.1.1",
        "merge-stream": "^1.0.1"
    },
    "scripts": {
        "prepublish": "gulp build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/antonlegoo/gitbook-plugin-theme-next"
    },
    "author": "Douglas Gabriel Cardoso <douglas@grupoartisan.com.br>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/nextgovbr/gitbook-plugin-theme-next/issues"
    },
    "gitbook": {
        "properties": {
            "styles": {
                "type": "object",
                "title": "Custom Stylesheets",
                "properties": {
                    "website": {
                        "title": "Stylesheet for website output",
                        "default": "styles/wikia.css"
                    }
                }
            },
            "showLevel": {
                "type": "boolean",
                "title": "Show level indicator in TOC",
                "default": false
            }
        }
    }
}
